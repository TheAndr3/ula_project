module unidade_xor_5x4 (
	input wire [4:0] a,
	input wire [3:0] b,
	output s
);

	wire gnd = 1'b0;
	wire x1, x2, x3, x4, x5;
	
	porta_xor xor_bit0 (
        .a(a[0]),
        .b(b[0]),
        .y(x1)
    );

    porta_xor xor_bit1 (
        .a(a[1]),
        .b(b[1]),
        .y(x2)
    );

    // XOR para o bit 2
    porta_xor xor_bit2 (
        .a(a[2]),
        .b(b[2]),
        .y(x3)
    );

    // XOR para o bit 3
    porta_xor xor_bit3 (
        .a(a[3]),
        .b(b[3]),
        .y(x4)
    );

    // XOR para o bit 4 (A[4] XOR 0)
    porta_xor xor_bit4 (
        .a(a[4]),
        .b(gnd), 
        .y(x5)
    );
	 
	 wire x12, x34, xx;
	 porta_xor x_12 (
		.a(x1), .b(x2),
		.y(x12)
		);
	
	 porta_xor x_34(
		.a(x3), .b(x4),
		.y(x34)
		);
	
	 porta_xor x_x(
		.a(x12), .b(x34),
		.y(xx)
		);
	
	 porta_xor xfinal(
		.a(xx), .b(x5),
		.y(s)
		);
endmodule